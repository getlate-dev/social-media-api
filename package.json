{
  "name": "@getlatedev/social-media-api",
  "version": "1.1.0",
  "description": "Drop-in replacement for the Ayrshare social-media-api SDK, powered by Late",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "social-media",
    "api",
    "scheduling",
    "twitter",
    "instagram",
    "facebook",
    "linkedin",
    "tiktok",
    "youtube",
    "late",
    "ayrshare"
  ],
  "author": "Late <support@getlate.dev>",
  "license": "MIT",
  "homepage": "https://getlate.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/getlate-dev/social-media-api"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
